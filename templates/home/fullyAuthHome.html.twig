{% extends 'fullyAuthLayout.html.twig' %}

{% block title %}
  Dashboard
{% endblock %}

{% block body %}
  <div class="px-4 py-6 rounded-md">
    <div class="grid grid-cols-1 md:lg:grid-cols-3 lg:grid-cols-5 gap-4 mb-4">

      <div class="flex items-center justify-center rounded-md bg-white border border-gray-200 dark:bg-gray-800 shadow-lg hover:shadow-md">
        <div class="self-center text-center justify-center size-full">
          <div class="p-3 text-2xl text-gray-400 dark:text-gray-500">
            <div class="flex justify-between">
              <p class="ml-2 text-black text-sm dark:text-white">Transactions terminées</p>
              <span class="relative py-2.5 px-5 bg-gray-200 rounded-xl text-gray-500 size-11">
                <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="absolute size-8 transform -translate-x-1/2 -translate-y-1/2 top-1/2 left-1/2">
                  <path stroke-linecap="round" stroke-linejoin="round" d="M7.5 21 3 16.5m0 0L7.5 12M3 16.5h13.5m0-13.5L21 7.5m0 0L16.5 12M21 7.5H7.5" />
                </svg>
              </span>
            </div>
            <div class="flex flex-col justify-center text-left">
              <p class="text-2xl font-bold text-gray-600 dark:text-gray-100">
                {{ totaleTransactionsAmount|format_currency('XOF', locale='fr') }}
              </p>
              <div class="flex justify-end text-sm text-green-500">
                <span class="text-gray-400 mr-2 text-xs">Les sept derniers jours</span>
                {% if totalTransactionPreviousLast7days > totaleTransactionsAmount %}
                  <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-6">
                    <path stroke-linecap="round" stroke-linejoin="round" d="M2.25 6 9 12.75l4.286-4.286a11.948 11.948 0 0 1 4.306 6.43l.776 2.898m0 0 3.182-5.511m-3.182 5.51-5.511-3.181" />
                  </svg>
                  <span>-{{ transactionPercentageByPreviousAnLast7Dys }}%</span>
                  {% else %}
                  <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="size-6">
                    <path stroke-linecap="round" stroke-linejoin="round" d="M2.25 18 9 11.25l4.306 4.306a11.95 11.95 0 0 1 5.814-5.518l2.74-1.22m0 0-5.94-2.281m5.94 2.28-2.28 5.941" />
                  </svg>
                  <span>+{{ transactionPercentageByPreviousAnLast7Dys }}%</span>
                {% endif %}
              </div>
            </div>
          </div>
        </div>
      </div>

      <div class="flex items-center justify-center rounded-md bg-white border border-gray-200 dark:bg-gray-800 shadow-lg hover:shadow-md">
        <div class="self-center text-center justify-center size-full">
          <div class="p-3 text-2xl text-gray-400 dark:text-gray-500">
            <div class="flex justify-between">
              <p class="ml-2 text-black text-sm dark:text-white">Service de pressing</p>
              <span class="relative py-2.5 px-5 bg-purple-200 rounded-xl text-purple-600 size-11">
